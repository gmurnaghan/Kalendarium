<p id="notice"><%= notice %></p>

<h1>Manuscripts</h1>

<table>
  <thead>
    <tr>
      <th>Shelfmark</th>
      <th>Name</th>
      <th>Calendar integral</th>
      <th>Numbering method</th>
      <th>Book type</th>
      <th>Manuscript or print</th>
      <th>Language</th>
      <th>Origin</th>
      <th>Origin note</th>
      <th>Destination</th>
      <th>Destination note</th>
      <th>Script</th>
      <th>Text height</th>
      <th>Text width</th>
      <th>Text depth</th>
      <th>Writing surface height</th>
      <th>Writing surface width</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Date note</th>
      <th>Extent</th>
      <th>Provenance</th>
      <th>State of completion</th>
      <th>Resource</th>
      <th>Columns</th>
      <th>Start folio</th>
      <th>End folio</th>
      <th>Color weighting</th>
      <th>Has shading</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manuscripts.each do |manuscript| %>
      <tr>
        <td><%= manuscript.shelfmark %></td>
        <td><%= manuscript.name %></td>
        <td><%= manuscript.calendar_integral %></td>
        <td><%= manuscript.numbering_method %></td>
        <td><%= manuscript.book_type %></td>
        <td><%= manuscript.manuscript_or_print %></td>
        <td><%= manuscript.language %></td>
        <td><%= manuscript.origin %></td>
        <td><%= manuscript.origin_note %></td>
        <td><%= manuscript.destination %></td>
        <td><%= manuscript.destination_note %></td>
        <td><%= manuscript.script %></td>
        <td><%= manuscript.text_height %></td>
        <td><%= manuscript.text_width %></td>
        <td><%= manuscript.text_depth %></td>
        <td><%= manuscript.writing_surface_height %></td>
        <td><%= manuscript.writing_surface_width %></td>
        <td><%= manuscript.start_year %></td>
        <td><%= manuscript.end_year%></td>
        <td><%= manuscript.date_note %></td>
        <td><%= manuscript.extent %></td>
        <td><%= manuscript.provenance %></td>
        <td><%= manuscript.state_of_completion %></td>
        <td><%= manuscript.resource %></td>
        <td><%= manuscript.columns %></td>
        <td><%= manuscript.start_folio %></td>
        <td><%= manuscript.end_folio %></td>
        <td><%= manuscript.color_weighting %></td>
        <td><%= manuscript.has_shading %></td>
        <td><%= link_to 'Show', manuscript %></td>
        <td><%= link_to 'Edit', edit_manuscript_path(manuscript) %></td>
        <td><%= link_to 'Destroy', manuscript, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Manuscript', new_manuscript_path %>
